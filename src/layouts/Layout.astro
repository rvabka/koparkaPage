---
import Navigation from "../components/Navigation.astro";
import "../styles/global.css";
import 'aos/dist/aos.css';

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="pl">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="Firma Transportowo-Usługowa Piotr Szymocha – profesjonalne usługi transportowe i ziemne. Oferujemy wykopy, budowę stawów, transport  i sprzedaż materiałów sypkich oraz drewna. Gwarantujemy najwyższą jakość i doświadczenie. Sprawdź naszą ofertę!" />
    <meta name="keywords" content="transport, usługi transportowe, wykopy, budowa stawów, sprzedaż drewna, materiały sypkie">
    <meta name="author" content="Firma Transportowo-Usługowa Piotr Szymocha">
    <meta property="og:title" content="Firma Transportowo-Usługowa Piotr Szymocha" />
    <meta property="og:description" content="Firma Transportowo-Usługowa Piotr Szymocha – profesjonalne usługi transportowe i ziemne. Oferujemy wykopy, budowę stawów, transport  i sprzedaż materiałów sypkich oraz drewna. Gwarantujemy najwyższą jakość i doświadczenie. Sprawdź naszą ofertę!" />
    <meta property="og:image" content="/photo1.webp" />
    <meta property="og:url" content="https://praceziemne-kozienice-radom.pl/" />
    <meta property="og:type" content="website" />
    <link rel="icon" type="image/png" href="/logo.png" />
    <title>{title}</title>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
  </head>
  <body>
    <Navigation />
    <main>
      <slot />
      <script>
        import AOS from "aos";
        AOS.init();
    </script>
    </main>

  </body>
</html>

